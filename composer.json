{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "2captcha/2captcha": "*",
        "aws/aws-sdk-php": "^3.247",
        "barryvdh/laravel-snappy": "^1.0",
        "guzzlehttp/guzzle": "^7.9",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "helvetitec/lagoon-charts": "~2.0",
        "ilovepdf/ilovepdf-php": "^1.2",
        "intervention/image": "^2.7",
        "itsgoingd/clockwork": "^5.1",
        "laravel/framework": "^11.38",
        "laravel/horizon": "^5.30",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.11",
        "laravel/tinker": "^2.9",
        "livewire/flux": "^1.1",
        "livewire/flux-pro": "^1.1",
        "livewire/livewire": "^3.5",
        "log1x/laravel-webfonts": "^1.0",
        "nesbot/carbon": "^3.0",
        "nyholm/psr7": "*",
        "opcodesio/log-viewer": "^3.4",
        "openai-php/client": "^0.10.3",
        "php-webdriver/webdriver": "^1.15",
        "railsware/mailtrap-php": "*",
        "regulus/tetra-text": "^0.6.16",
        "setasign/fpdf": "1.8.*",
        "setasign/fpdi": "^2.3",
        "spatie/browsershot": "^5.0",
        "spatie/image": "^3.0",
        "spatie/simple-excel": "^3.6",
        "symfony/dom-crawler": "^7.0",
        "symfony/http-client": "*",
        "symfony/mailgun-mailer": "^7.0",
        "symfony/panther": "^2.1",
        "twilio/sdk": "^6.44",
        "typesense/typesense-php": "^4.9",
        "vlucas/phpdotenv": "^5.5",
        "zoon/puphpeteer": "^2.2"
    },
    "require-dev": {
        "dbrekelmans/bdi": "^1.0",
        "fakerphp/faker": "^1.23",
        "laravel/breeze": "^2.0",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "pre-autoload-dump": [],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "process-timeout": 0,
        "allow-plugins": {
            "php-http/discovery": true,
            "tbachert/spi": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": {
        "flux-pro": {
            "type": "composer",
            "url": "https://composer.fluxui.dev"
        }
    }
}
